{
    "name": "cronnor",
    "version": "0.4.0",
    "description": "Bibliothèque JavaScript implémentant un programme Unix cron.",
    "keywords": [
        "cron",
        "crontab",
        "scheduler",
        "cronnor",
        "front-end",
        "backend"
    ],
    "homepage": "https://github.com/regseb/cronnor#readme",
    "bugs": {
        "url": "https://github.com/regseb/cronnor/issues"
    },
    "license": "MIT",
    "author": "Sébastien Règne <regseb@gmail.com> (https://github.com/regseb)",
    "files": [
        "src/",
        "types/"
    ],
    "main": "src/cron.js",
    "types": "types/cron.js",
    "repository": "regseb/cronnor",
    "funding": "https://www.paypal.me/sebastienregne",
    "type": "module",
    "scripts": {
        "lint": "metalint",
        "tsc:lint": "tsc",
        "tsc:build": "tsc -p tsconfig.build.json",
        "test": "c8 mocha test/",
        "coverage:html": "c8 report --reporter html",
        "coverage:lcov": "c8 report --reporter lcovonly",
        "jsdocs": "jsdoc -r -a all -d jsdocs/ src/",
        "release": "standard-version",
        "clean": "rm -rf coverage/ jsdocs/ node_modules/"
    },
    "devDependencies": {
        "@types/mocha": "^8.0.3",
        "@types/node": "^10.12.18",
        "@types/sinon": "^9.0.5",
        "c8": "^7.3.5",
        "david": "^12.0.0",
        "eslint": "^7.16.0",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "^30.7.9",
        "eslint-plugin-mocha": "^8.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-unicorn": "^24.0.0",
        "jsdoc": "^3.6.6",
        "jsonlint-mod": "^1.7.6",
        "markdownlint": "^0.22.0",
        "metalint": "^0.9.0",
        "mocha": "^8.2.1",
        "sinon": "^9.2.2",
        "standard-version": "^9.0.0",
        "typescript": "^4.0.2",
        "yaml-lint": "^1.2.4"
    },
    "engines": {
        "node": ">=14.0.0"
    }
}
