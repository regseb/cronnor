{
    "name": "cronnor",
    "version": "1.1.0",
    "description": "Bibliothèque JavaScript implémentant un programme Unix cron.",
    "keywords": [
        "cron",
        "crontab",
        "scheduler",
        "cronnor",
        "front-end",
        "backend"
    ],
    "homepage": "https://github.com/regseb/cronnor#readme",
    "bugs": {
        "url": "https://github.com/regseb/cronnor/issues"
    },
    "license": "MIT",
    "author": "Sébastien Règne <regseb@gmail.com> (https://github.com/regseb)",
    "funding": "https://www.paypal.me/sebastienregne",
    "files": [
        "dist/",
        "types/"
    ],
    "main": "dist/cron.js",
    "browser": "dist/cron.js",
    "types": "types/cron.js",
    "repository": "regseb/cronnor",
    "type": "module",
    "scripts": {
        "lint": "metalint",
        "tsc:lint": "tsc",
        "test": "c8 mocha test/",
        "coverage:html": "c8 report --reporter html",
        "coverage:lcov": "c8 report --reporter lcovonly",
        "jsdocs": "jsdoc -r -a all -d jsdocs/ src/",
        "bundle": "node .script/bundle",
        "declare": "tsc -p tsconfig.build.json",
        "prepare": "npm run bundle && npm run declare",
        "release": "standard-version",
        "clean": "node .script/clean.js"
    },
    "devDependencies": {
        "@types/mocha": "^8.2.1",
        "@types/node": "^14.14.33",
        "@types/sinon": "^9.0.11",
        "c8": "^7.6.0",
        "david": "^12.0.0",
        "eslint": "^7.21.0",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "^32.2.0",
        "eslint-plugin-mocha": "^8.1.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.3.1",
        "eslint-plugin-unicorn": "^28.0.2",
        "jsdoc": "^3.6.6",
        "jsonlint-mod": "^1.7.6",
        "markdownlint": "^0.23.1",
        "metalint": "^0.9.0",
        "mocha": "^8.3.1",
        "rollup": "^2.41.2",
        "rollup-plugin-terser": "^7.0.2",
        "sinon": "^9.2.4",
        "standard-version": "^9.1.1",
        "typescript": "^4.2.3",
        "yaml-lint": "^1.2.4"
    },
    "engines": {
        "node": ">=14.13.0"
    }
}
