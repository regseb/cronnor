{
    "name": "cronnor",
    "version": "1.2.1",
    "description": "Bibliothèque JavaScript implémentant un programme cron.",
    "keywords": [
        "cron",
        "crontab",
        "scheduler",
        "cronnor",
        "front-end",
        "backend"
    ],
    "homepage": "https://github.com/regseb/cronnor#readme",
    "bugs": {
        "url": "https://github.com/regseb/cronnor/issues",
        "email": "regseb@gmail.com"
    },
    "license": "MIT",
    "author": "Sébastien Règne <regseb@gmail.com> (https://github.com/regseb)",
    "funding": "https://www.paypal.me/sebastienregne",
    "files": [
        "index.js",
        "index.d.ts"
    ],
    "main": "index.js",
    "types": "index.d.ts",
    "repository": "regseb/cronnor",
    "type": "module",
    "scripts": {
        "lint": "metalint",
        "lint:types": "tsc",
        "test": "stryker run",
        "test:mocha": "mocha",
        "jsdocs": "typedoc --tsconfig tsconfig_typedoc.json --out jsdocs/ src/",
        "bundle": "rollup src/index.js --file index.js",
        "declare": "tsc index.js --declaration --allowJs --emitDeclarationOnly",
        "prepare": "npm run bundle && npm run declare",
        "release": "standard-version",
        "clean": "node .script/clean.js"
    },
    "devDependencies": {
        "@stryker-mutator/core": "^5.4.1",
        "@stryker-mutator/mocha-runner": "^5.4.1",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.7",
        "@types/sinon": "^10.0.6",
        "eslint": "^8.2.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsdoc": "^37.0.3",
        "eslint-plugin-mocha": "^9.0.0",
        "eslint-plugin-no-unsanitized": "^4.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-regexp": "^1.5.0",
        "eslint-plugin-unicorn": "^38.0.1",
        "jsonlint-mod": "^1.7.6",
        "markdownlint": "^0.24.0",
        "metalint": "^0.9.0",
        "mocha": "^9.1.3",
        "rollup": "^2.60.0",
        "sinon": "^12.0.1",
        "standard-version": "^9.3.2",
        "typedoc": "^0.22.8",
        "typescript": "^4.4.4",
        "yaml-lint": "^1.2.4"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
